<%-include("header")-%>
    <h1>Edit</h1>
    <form name="" action="/" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" name="title" class="form-control" id="title"  value="<%=title%>">

  </div>
  <div class="form-group">
    <label for="exampleFormControlTextarea1">Your Content</label>
    <textarea name="content" class="form-control" id="exampleFormControlTextarea1" rows="15" ><%=content%></textarea>
  </div>
  <button name="id" value="<%=id%>" type="submit" class="btn btn-primary">Save</button>
</form>
<%-include("footer")-%>
